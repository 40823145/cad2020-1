document.addEventListener("copy",function(f){var g=getSelectionSnippet();if(g){var c=window.getSelection().toString();var b="";if(g.classList.contains("cs")||g.classList.contains("cpp")){b="//"}else{if(g.classList.contains("vb")||g.classList.contains("vba")){b="'"}else{return}}var d=b+"**********************\r\n"+b+"Copyright(C) 2020 Xarial Pty Limited\r\n"+b+"Reference: "+window.location+"\r\n"+b+"License: https://www.codestack.net/license/\r\n"+b+"**********************\r\n\r\n";var a=f.clipboardData||window.clipboardData;a.setData("Text",d+c);f.preventDefault()}});function getSnippetNode(a){while(a){if((a.className)&&a.className.includes("snippet")){return a}a=a.parentNode}return undefined}function getSelectionSnippet(){var b;if(window.getSelection){b=window.getSelection();if(b.rangeCount>0){for(var a=0;a<b.rangeCount;++a){return getSnippetNode(b.getRangeAt(a).commonAncestorContainer)}return undefined}}else{if((b=document.selection)&&b.type!="Control"){return getSnippetNode(b.createRange().parentElement())}}return undefined};
